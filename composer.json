{
    "name": "im-wmkong/laravel-model-filter",
    "description": "Laravel Eloquent Models filter",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "filter",
        "query"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "WmKong",
            "email": "im.wmkong@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModelFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModelFilter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.24"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --using-cache=no --diff --dry-run --ansi",
        "fix-style": "php-cs-fixer fix --using-cache=no --ansi",
        "test": "vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ModelFilter\\ServiceProvider"
            ]
        },
        "hooks": {
            "pre-commit": [
                "composer fix-style"
            ],
            "pre-push": [
                "composer test"
            ]
        }
    }
}
